<!DOCTYPE html>
<html>
<head>
<title><%= title %></title>
<%= stylesheet_link_tag  "application", media: "all" %>
<%#= stylesheet_link_tag  "jquery-jvectormap-1.2.2.css" %>
<%= javascript_include_tag "jquery-1.9.1.js" %>
<%= javascript_include_tag "application" %>
<%= javascript_include_tag "jquery-jvectormap-1.2.2.min.js" %>
<%= javascript_include_tag "jquery-jvectormap-world-mill-en.js" %>
<%= csrf_meta_tags %>
<%= render 'layouts/shim' %>
</head>
<body>
<%= render 'layouts/header' %>

<div class="container">
  <% if notice %>
    <div class="alert alert-success">
      <button type="button" class="close" data-dismiss="alert">&times;</button>
      <p><%= notice %></p>
    </div>
  <% end %>

  <% if alert %>
    <div class="alert alert-danger">
      <button type="button" class="close" data-dismiss="alert">&times;</button>
      <p><%= alert %></p>
    </div>
  <% end %>
<%= yield %>

</div>
<%= render 'layouts/footer' %>
<%#= debug(params) if Rails.env.development? %>
</body>
</html>
