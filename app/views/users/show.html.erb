<% provide(:title, @user.name) %>
<div class="container">
  <div class="btn-group btn-group-justified">
    <%= link_to @title + " : $0.00", "", class:"btn"%>
    <%= link_to "1Q : $0.00", "", class:"btn"%>
    <%= link_to "2Q : $0.00", "", class:"btn"%>
    <%= link_to "3Q : $0.00", "", class:"btn"%>
    <%= link_to "4Q : $0.00", "", class:"btn"%>
  </div>

  <div class="btn-group btn-group-justified">
    <%= link_to "Main View", "", class:"btn"%>
    <%= link_to "Chart View", "", class:"btn"%>
    <%= link_to "Detailed View", "", class:"btn"%>
  </div>

  <table class="table table-bordered">
    <%= render 'shared/summary' %>
  </table>
</div>
